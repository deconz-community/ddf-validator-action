{
  "name": "ddf-validator-action",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "up": "taze major -I",
    "all": "pnpm run build && pnpm run lint && pnpm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deconz-community/ddf-validator-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@deconz-community/ddf-validator": "^1.2.0",
    "glob": "^9.3.0",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.36.0",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^27.2.1",
    "taze": "^0.9.0",
    "typescript": "^4.9.5"
  }
}